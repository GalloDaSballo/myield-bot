{
  "name": "typescript-package-starter",
  "description": "A simple Typescript template to start your package",
  "author": {
    "name": "Alex The Entreprenerd",
    "url": "https://github.com/GalloDaSballo",
    "email": "alex@entreprenerd.xyz"
  },
  "version": "1.1.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node lib/server",
    "dev": "yarn build && yarn start",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/. --ext ts --ext tsx --ext js",
    "lint:fix": "eslint src/. --ext ts --ext tsx --ext js --fix",
    "doc": "tsc && jsdoc -c ./.jsdoc.json"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "ethers": "^5.1.4",
    "express": "^4.17.1",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
